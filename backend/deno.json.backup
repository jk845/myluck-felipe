      "deno.window"
    ],
    "strict": true
  },
  "nodeModulesDir": "auto",
  "imports": {
    "@supabase/supabase-js": "https://esm.sh/@supabase/supabase-js@2.49.9",
    "@supabase/functions-js": "jsr:@supabase/functions-js",
    "@mollie/api-client": "npm:@mollie/api-client@^4.1.0",
    "resend": "npm:resend@latest",
